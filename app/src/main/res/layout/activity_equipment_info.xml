<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yunweiqiang.pingterest.EquipmentInfo">


    <TextView
        android:id="@+id/ItemName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ItemName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="85dp" />

    <TextView
        android:id="@+id/ItemDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ItemDescription"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="399dp" />

    <TextView
        android:id="@+id/ItemPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="ItemPrice"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="58dp" />

    <TextView
        android:id="@+id/Seller"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seller"
        tools:layout_editor_absoluteX="189dp"
        tools:layout_editor_absoluteY="399dp" />

    <Button
        android:id="@+id/equipmentChat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="483dp" />

    <TextView
        android:id="@+id/ItemBrand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ItemBrand"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="431dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="299dp"
        android:layout_height="260dp"
        app:srcCompat="@drawable/test1"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="120dp" />
</android.support.constraint.ConstraintLayout>
